{
    "name": "Aero-ts Project",
    "servePort": 8000,
    "partitions": {
		"include": {
			"path": "include",
			"target": "ReplicatedStorage.RobloxTS.Include"
		},
		"modules": {
			"path": "modules",
			"target": "ReplicatedStorage.RobloxTS.Modules"
		},
        "Server": {
            "path": "out/Server",
            "target": "ServerScriptService.Aero"
        },
        "Client": {
            "path": "out/Client",
            "target": "StarterPlayer.StarterPlayerScripts.Aero"
        },
        "Shared": {
            "path": "out/Shared",
            "target": "ReplicatedStorage.Aero"
        }
    }
}